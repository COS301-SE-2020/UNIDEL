@{
    ViewData["Title"] = "Driver current location";
}
<body class="splash">
    <div class="container shadow p-3 mb-5 bg-white rounded">
        <div class="center">
            <div class="text-center">
                <h1 class="display-4">Drivers Location</h1>
            </div>

            <iframe class="widthy heighty"
                    
                    frameborder="0" style="border:0"
                    src=@Model allowfullscreen>
            </iframe>
        </div>
    </div>
</body>

